{
    "name": "gresci/aruba-sms",
    "description": "Aruba SMS Notifications channel for Laravel 5.3.",
    "keywords": ["laravel", "notifications", "smscentre", "smsc", "aruba"],
    "homepage": "http://www.cluweb.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Giorgio Resci",
            "role": "Developer",
            "email": "giorgio@cluweb.com",
            "homepage": "http://www.cluweb.com/"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/queue": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\ArubaSMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ArubaSMS\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
